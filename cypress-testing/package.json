{
  "name": "cypress-testing",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec \"**/*.feature\"",
    "test:all": "cypress run --spec \"**/*.features\"",
    "test:chrome": "cypress run --spec \"**/*.feature\" --browser chrome --headless",
    "test:firefox": "cypress run --spec \"**/*.feature\" --browser firefox --headless",
    "docs": "typedoc --excludePrivate",
    "full-docs": "typedoc",
    "open": "cypress open",
    "compile": "tsc --noEmit",
    "checkLint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "keywords": [
    "mlsb",
    "testing",
    "softball",
    "cypress",
    "cucumber"
  ],
  "author": "Dallas Fraser",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@testing-library/cypress": "^8.0.2",
    "@types/node": "^13.9.8",
    "@types/testing-library__cypress": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "axe-core": "^4.3.5",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "cypress": "9.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.65.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "step_definitions": "cypress/features"
  },
  "dependencies": {
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/uuid": "^8.3.0",
    "cypress-axe": "^0.14.0",
    "install": "^0.13.0",
    "md5-base64": "0.0.3",
    "npm": "^6.14.11",
    "oauth-1.0a": "^2.2.6",
    "path-browserify": "^1.0.1",
    "typedoc": "^0.20.15",
    "uuid": "^8.3.2"
  }
}
